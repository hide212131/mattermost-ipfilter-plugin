{
    "id": "hide212131.mattermost-plugin-attachment-filter",
    "name": "Attachment Filter Plugin",
    "description": "This plugin provides a filter function to allow or deny about attempting to upload attachments.",
    "homepage_url": "https://github.com/hide212131/mattermost-plugin-attachment-filter",
    "support_url": "https://github.com/hide212131/mattermost-plugin-attachment-filter",
    "release_notes_url": "https://github.com/hide212131/mattermost-plugin-attachment-filter",
    "icon_path": "assets/starter-template-icon.svg",
    "version": "0.1.0",
    "min_server_version": "5.12.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "header": "",
        "footer": "",
        "settings": [{
          "key": "AllowedIps",
          "display_name": "Allowed IP Addresses",
          "type": "text",
          "help_text": "Specify the IP address to be Allowed, using forward matching and comma-separated characters, such as \"192.168.\\*, 127.\\*\".",
          "default": ""
        }]
    }
}
